{
  "name": "snake/framework",
  "description": "Sea Snake Framework is a PHP framework for building web applications.",
  "keywords": ["sea snake", "snake", "php"],
  "license": "MIT",
  "homepage": "https://github.com/Sleepy4k/php-sea-snake-fw",
  "support": {
    "issues": "https://github.com/Sleepy4k/php-sea-snake-fw/issues",
    "source": "https://github.com/Sleepy4k/php-sea-snake-fw/framework"
  },
  "authors": [
    {
      "name": "Apri Pandu Wicaksono",
      "email": "sarahpalastrin@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2"
  },
  "autoload": {
    "files": [
      "src/Helper/config.php",
      "src/Helper/debug.php",
      "src/Helper/file.php",
      "src/Helper/hash.php",
      "src/Helper/input.php",
      "src/Helper/password.php",
      "src/Helper/redirect.php",
      "src/Helper/session.php",
      "src/Helper/url.php",

      "src/Exeception/ErrorHandler.php",
      "src/Exeception/ExceptionHandler.php"
    ],
    "psr-4": {
      "Snake\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
